{
  "name": "prerender-clone",
  "version": "1.0.0",
  "description": "Express server that caches prerendered pages via Redis and RabbitMQ",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
