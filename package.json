    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "puppeteer": "^24.34.0",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/puppeteer": "^5.4.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
